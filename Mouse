public Mouse(String numSerie, String fabricante, String cor) {
    this.numSerie = numSerie;
    this.fabricante = fabricante;
    this.cor = cor;
}

public void imprimeMouse() {
    System.out.println("Número de Série:" +numSerie);
    System.out.println("Fabricante" + fabricante);
    System.out.println("Cor" + cor);
}
//construtor


public void setNumSerie(String numSerie) { //nO SET E GET TEM QUE SER MAIUSCULA O COMECO
    this.numSerie = numSerie;
}
public String getNumSerie(){
    return numSerie;
}

public void setFabricante(String fabricante) {
    this.fabricante = fabricante;
}
public String getFabricante(){
    return fabricante;
}

public void setCor(String cor) {
    this.cor = cor;
}
public String getCor(){
    return cor;
}
}


